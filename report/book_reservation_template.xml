<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="report_book_reservation">
        <t t-foreach="docs" t-as="o">
            <t t-call="web.html_container">
                <t t-call="web.external_layout">
                    <div class="page">
<!--                        <h2>-->
<!--                            <span t-field="property.name"/>-->
<!--                        </h2>-->
<!--                        <div>-->
<!--                            <strong>Expected Price: </strong>-->
<!--                            <span t-field="property.expected_price"/>-->
<!--                        </div>-->
                        <table class="table">
                            <thead>
                                <tr>
                                    <th>Product</th>
                                    <th>Internal Reference</th>
                                    <th>Price</th>

                                </tr>
                            </thead>
                            <tbody>
                                <t t-set="book" t-value="o.mapped('book_ids')"/>
                                <tr t-foreach="book" t-as="bk">
                                    <td>
                                        <span t-field="bk.product_id"/>
                                    </td>
                                    <td>
                                        <span t-field="bk.default_code"/>
                                    </td>
                                    <td>
                                        <span t-field="bk.unit_price"/>
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </t>
            </t>
        </t>
    </template>
</odoo>
